{
  "name": "prospecting-engine",
  "version": "2.0.0",
  "description": "Universal company discovery and enrichment system",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:setup": "node database/setup.js",
    "db:setup:dry": "node database/setup.js --dry-run",
    "test": "node --test tests/**/*.js",
    "test:unit": "node --test tests/unit/**/*.js",
    "test:integration": "node --test tests/integration/**/*.js",
    "test:discovery": "node tests/unit/test-google-maps.js",
    "test:extraction": "node tests/unit/test-extraction.js",
    "test:pipeline": "node tests/integration/test-full-pipeline.js",
    "test:phase-4": "node tests/integration/test-phase-4-intelligence.js",
    "test:e2e": "node tests/integration/test-end-to-end.js",
    "test:comprehensive": "node tests/integration/test-comprehensive.js",
    "utils:check-quality": "node tests/utils/check-data-quality.js",
    "utils:fix-data": "node tests/utils/fix-bad-data.js"
  },
  "keywords": [
    "prospecting",
    "lead-generation",
    "business-discovery",
    "google-maps",
    "web-scraping"
  ],
  "author": "Maxant Agency",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "@supabase/supabase-js": "^2.75.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.75.0",
    "playwright": "^1.40.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
