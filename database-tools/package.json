{
  "name": "database-tools",
  "version": "1.0.0",
  "description": "CLI utility to automate Supabase database setup from JSON schemas",
  "type": "module",
  "main": "cli.js",
  "bin": {
    "db": "./cli.js"
  },
  "scripts": {
    "db:setup": "node cli.js setup",
    "db:migrate": "node cli.js migrate",
    "db:seed": "node cli.js seed",
    "db:validate": "node cli.js validate",
    "db:generate": "node cli.js generate",
    "backup:stats": "node scripts/backup-stats.js",
    "backup:stats:watch": "node scripts/backup-stats.js --watch",
    "backup:stats:json": "node scripts/backup-stats.js --json",
    "backup:retry": "node scripts/retry-failed-uploads.js",
    "backup:cleanup": "node scripts/cleanup-old-backups.js",
    "backup:validate": "node scripts/validate-existing-backups.js"
  },
  "keywords": [
    "database",
    "supabase",
    "cli",
    "schema",
    "migration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  }
}
